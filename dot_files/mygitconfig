[credential]
	helper = store --file ~/.git/.credentials
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[merge]
	tool = vimdiff
[alias]
   pl = log --graph --format=oneline --abbrev-commit --decorate --all
[push]
	default = simple
[diff "zip"]
	textconv = unzip -c -a
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
